[unix_http_server]
file=/var/run/supervisor.sock    ; (the path to the socket file)
chmod=0770  ; (socket file mode, default: 0700)
chown=<%=supervisord_user%>:<%=supervisord_user%>

[inet_http_server]
port=9001

[supervisord]
user=<%=supervisord_user%>
logfile=<%=supervisord_log_dir%>/supervisord.log          ; (main log file;default $CWD/supervisord.log)
logfile_maxbytes=5MB                                      ; (max main logfile bytes b4 rotation;default 50MB)
logfile_backups=30                                        ; (num of main logfile rotation backups;default 10)
loglevel=info                                             ; (log level;default info; others: debug,warn,trace)
pidfile=/var/run/supervisord.pid                          ; (supervisord pidfile;default supervisord.pid)
childlogdir=/var/log/apps                                 ; ('AUTO' child log dir, default $TEMP)
nocleanup=false
umask=022

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl=unix:///var/run/supervisor.sock ; use a unix:// URL  for a unix socket

[include]
files = /etc/supervisord.d/*.ini

[program:<%=binary_name%>]
command=<%=binary_file%> <%=config_file%>
autorestart=true
startsecs=20
stdout_logfile=<%=log_dir%>/<%=binary_name%>.log
stdout_logfile_maxbytes=5MB
stdout_logfile_backups=30
redirect_stderr=true
stderr_logfile=<%=log_dir%>/<%=binary_name%>.err
stderr_logfile_maxbytes=5MB
stderr_logfile_backups=30
